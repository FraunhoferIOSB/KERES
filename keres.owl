    <owl:Class rdf:about="https://raw.githubusercontent.com/FraunhoferIOSB/KERES/main/keres.owl#ClimateDataManagementSystem">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/FraunhoferIOSB/KERES/main/keres.owl#ITSystem"/>
