        <rdf:type rdf:resource="https://raw.githubusercontent.com/FraunhoferIOSB/KERES/main/keres.owl#Domain"/>
        <rdfs:label xml:lang="en">Domain “Climate Change”</rdfs:label>
        <rdfs:label xml:lang="de">Domäne „Klimawandel“</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://raw.githubusercontent.com/FraunhoferIOSB/KERES/main/keres.owl#Domain_RiskAssessment -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/FraunhoferIOSB/KERES/main/keres.owl#Domain_RiskAssessment">
