        <rdfs:label xml:lang="en">Neustadt an der Aisch-Bad Windsheim</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://raw.githubusercontent.com/FraunhoferIOSB/KERES/main/keres.owl#District_Oberhavel -->

