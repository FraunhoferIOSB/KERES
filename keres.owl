    <!-- https://github.com/FraunhoferIOSB/KERES/blob/main/keres.owl#IndirectEconomicValue -->

    <owl:Class rdf:about="https://github.com/FraunhoferIOSB/KERES/blob/main/keres.owl#IndirectEconomicValue">
        <rdfs:subClassOf rdf:resource="https://github.com/FraunhoferIOSB/KERES/blob/main/keres.owl#CulturalHeritageValue"/>
        <rdfs:comment>&lt;p&gt;
