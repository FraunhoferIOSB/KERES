        <Threat_canCause_Damage rdf:resource="https://raw.githubusercontent.com/FraunhoferIOSB/KERES/main/keres.owl#Erosion_Prototype"/>
        <Threat_canCause_Damage rdf:resource="https://raw.githubusercontent.com/FraunhoferIOSB/KERES/main/keres.owl#Weathering_Prototype"/>
