        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/FraunhoferIOSB/KERES/main/keres.owl#ThreatFromHumanActivity"/>
        <rdfs:label xml:lang="en">Physical Resource Extraction</rdfs:label>
        <rdfs:label xml:lang="de">Ressourcengewinnung</rdfs:label>
