    <!-- https://github.com/FraunhoferIOSB/KERES/blob/main/keres.owl#Lighting -->

    <owl:Class rdf:about="https://github.com/FraunhoferIOSB/KERES/blob/main/keres.owl#Lighting">
        <rdfs:subClassOf rdf:resource="https://github.com/FraunhoferIOSB/KERES/blob/main/keres.owl#Maintenance"/>
        <rdfs:label xml:lang="de">Beleuchtung</rdfs:label>
