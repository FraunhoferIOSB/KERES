        <Material_hasIntendedUseAs_MaterialUse rdf:resource="https://github.com/FraunhoferIOSB/KERES/blob/main/keres.owl#MaterialPurpose_StructuralMaterial"/>
        <Material_hasProperty_MaterialProperty rdf:resource="https://github.com/FraunhoferIOSB/KERES/blob/main/keres.owl#StandardCompressiveStrength_LimestoneCompressiveStrength"/>
